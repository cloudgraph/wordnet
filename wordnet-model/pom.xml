		</plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>2.12.4</version>
                <executions>
                   <execution>
                    <id>all-tests</id>
                    <goals>
                      <goal>test</goal>
                    </goals>
                    <configuration>
	                    <skip>false</skip>
	                    <additionalClasspathElements>
	                        <additionalClasspathElement>${basedir}/src/main/resources</additionalClasspathElement>
	                        <additionalClasspathElement>${basedir}/src/test/resources</additionalClasspathElement>
	                    </additionalClasspathElements>                  
	                    <forkMode>once</forkMode>
	                    <systemProperties>
	                        <property><name>plasma.configuration</name><value>plasma-config.xml</value></property>
	                        <property><name>fuml.configuration</name><value>fuml-config.xml</value></property>
	                        <property><name>log4j.configuration</name><value>log4j.properties</value></property>
	                    </systemProperties>
	                    
	                    <excludes>
	                     <exclude>**/*DictionaryExportTest*</exclude>
<!--                          <exclude>**/*DictionaryQueryTest*</exclude> -->
	                    </excludes>
                    </configuration>
                  </execution>
                     
                </executions>                
